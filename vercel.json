{

      "builds": [{
          "src": "newproject/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.0" }
      }],
      "routes": [
          {
              "src": "/(.*)",
              "dest": "newproject/wsgi.py"
          }
      ]
}